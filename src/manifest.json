{
  "package": "moe.orpu.omusic",
  "name": "OMusic",
  "versionName": "2.0",
  "versionCode": 200,
  "minPlatformVersion": 1000,
  "icon": "/common/icon.png",
  "deviceTypeList": [
    "watch"
  ],
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.audio"
    },
    {
      "name": "system.request"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.file"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.vibrator"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.app"
    },
    {
      "name": "system.network"
    },
    {
      "name": "system.interconnect"
    },
    {
      "name": "system.crypto"
    },
    {
      "name": "system.brightness"
    }
  ],
  "config": {
    "logLevel": "log",
    "designWidth": "device-width",
    "background": {
      "features": [
        "system.audio",
        "system.request"
      ]
    }
  },
  "display": {
    "backgroundColor": "#000000"
  },
  "router": {
    "entry": "pages/splash",
    "pages": {
      "pages/player": {
        "component": "player",
        "launchMode": "singleTask"
      },
      "pages/volume": {
        "component": "volume"
      },
      "pages/list": {
        "component": "list"
      },
      "pages/search": {
        "component": "search"
      },
      "pages/menu": {
        "component": "menu"
      },
      "pages/user": {
        "component": "user"
      },
      "pages/settings": {
        "component": "settings"
      },
      "pages/storage": {
        "component": "storage"
      },
      "pages/login": {
        "component": "login"
      },
      "pages/about": {
        "component": "about"
      },
      "pages/license": {
        "component": "license"
      },
      "pages/compileInfo": {
        "component": "compileInfo"
      },
      "pages/songlist_list": {
        "component": "songlist_list"
      },
      "pages/splash": {
        "component": "splash"
      }
    },
      "permissions": [
        { "name": "hapjs.permission.DEVICE_INFO" }
      ]
    
  }
}